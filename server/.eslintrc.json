{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-undef": "error",
		"comma-dangle": "error",
		"brace-style": "error",
		"comma-spacing": "error",
		"array-bracket-spacing": "error",
		"no-mixed-spaces-and-tabs": "error",
		"no-duplicate-imports": "error",
		"no-console": "warn",
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"semi": ["error", "always"]
	}
}
